package parkingLot.com.parkingLot;

public class Car {

	private String color;
	private String RegNum;
	
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public String getRegNum() {
		return RegNum;
	}
	public void setRegNum(String regNum) {
		RegNum = regNum;
	}
	
	
}
